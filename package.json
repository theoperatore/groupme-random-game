{
  "name": "groupme-random-game",
  "version": "1.0.0",
  "description": "Once a day, send a random game to the groupme chat",
  "main": "build/server.js",
  "repository": "https://github.com/theoperatore/groupme-random-game.git",
  "author": "Alex Petersen <theoperatore@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build:old": "babel src -d build",
    "build": "tsc",
    "start": "forever -d -a -o out.log -e err.log start build/server.js",
    "stop": "forever stop build/server.js",
    "dev": "ts-node src/server.ts",
    "logs": "forever logs 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@theoperatore/alorg-service": "^2.1.1",
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "cors": "^2.8.4",
    "dotenv": "^7.0.0",
    "express": "^4.15.4",
    "helmet": "^3.15.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/compression": "^0.0.36",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.1",
    "@types/helmet": "^0.0.43",
    "husky": "^1.3.1",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.4"
  }
}
